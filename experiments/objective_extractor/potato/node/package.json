{
    "name": "potato-node",
    "version": "1.0.0",
    "description": "A set of modules to make interfacing javascript with potato easier.",
    "main": "./live/potato.umd.js",
    "module": "./live/potato.js",
    "type": "module",
    "exports": {
        ".": {
            "import": "./live/potato.js",
            "require": "./live/potato.umd.cjs"
        }
    },
    "files": [
        "live"
    ],
    "scripts": {
        "dev": "tsc && vite build --watch",
        "build": "tsc && vite build",
        "preview": "vite preview"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/davidjurgens/potato.git"
    },
    "keywords": [
        "potato",
        "text",
        "annotation",
        "utility",
        "ai",
        "learning",
        "model",
        "crowdsourcing",
        "collaboration"
    ],
    "author": "David Jurgens and Co.",
    "license": "SEE LICENSE IN LICENSE",
    "bugs": {
        "url": "https://github.com/davidjurgens/potato/issues"
    },
    "homepage": "https://github.com/davidjurgens/potato#readme",
    "devDependencies": {
        "@types/jquery": "^3.5.31",
        "typescript": "^5.5.3",
        "vite": "^5.4.8"
    },
    "dependencies": {
        "@flatten-js/interval-tree": "^1.1.3",
        "@types/node": "^22.7.4",
        "trie-search": "^2.0.0"
    }
}
